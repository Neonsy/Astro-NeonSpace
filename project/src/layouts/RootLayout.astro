---
import Head from '@components/Head.astro';

import '@styles/global.css';

type Props = {
    children: any;
    title: string;
    description: string;
};

const { title, description } = Astro.props;
---

<!doctype html>
<html lang='en'>
    <Head title={title} description={description} />
    <body>
        <slot />
    </body>
</html>
